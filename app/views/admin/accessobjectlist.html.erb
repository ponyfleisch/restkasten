<h1>access object list</h1>
<table class="table table-striped">
  <tr>
    <th>name</th>
    <th>actions</th>
  </tr>
  <tr ng-repeat="object in accessObjects">
    <td>
      {{object.name}}
    </td>
    <td>
      <a ng-href="/access_objects/{{object.id}}" class="btn btn-primary"><span class="glyphicon glyphicon-pencil"></span></a>
    </td>
  </tr>
</table>
